{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "files.eol": "\n",
  "editor.tabSize": 2,
  "typescript.tsdk": "node_modules/typescript/lib",
  "stylelint.validate": ["less", "sass", "scss", "css"]
}
